
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/YeniCore_Ynt1mPnl/Views/Shared/YonetimPanelAnaKatman.cshtml";
}


@model List<LiveMedy.Models.LiveMedyYntUygulamaKontrol>


<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4>UYGULAMA KONTROL LOG KAYITLARI</h4>
            </div>
            <div class="panel-body">
                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr>
                            <th>Uygulama</th>
                            <th>StatusCode</th>
                            <th>Açıklama</th>
                            <th>Tarih</th>
                            <th>Kullanıcı</th>
                        </tr>
                    </thead>
                    <tbody>

                        @foreach (var item in Model)
                        {
                            <tr id="tr_@item.KontrolSonucId">
                                <td>@item.Uygulama.UygulamaAdi</td>
                                <td>@item.StatusCode</td>
                                <td>@item.Aciklama</td>
                                <td>@item.Tarih</td>
                                <td>@item.Kullanici.KullaniciAdi</td>

                            </tr>
                        }
                    </tbody>
                </table>
            </div>


        </div>
    </div>
</div>


<link href="~/css/core.css" rel="stylesheet" />
<script src="~/js/core.js"></script>